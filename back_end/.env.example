# Copy this file to `.env` and fill in the required secrets.
ADMIN_PASSWORD=changeme

# Database credentials (existing deployment already expects these).
DB_USER=example_user
DB_PASS=example_password
DB_HOST=db.host.example.com
DB_NAME=orders_db

# RabbitMQ / Amazon MQ (AMQP) connection settings.
# Switch between local Docker and AWS MQ by pointing these to the right broker.
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE_NAME=order_write_jobs
